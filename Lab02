Problem 1

#include <stdio.h>      // allows use of printf and scanf functions

int main() {
        int c, f = 50;          // declare variables, t is also initialized

        printf("Enter a value for f: ");
        scanf("%d", &f);

        c = 5 * (f - 32) / 9;   // expression evaluates "x"

        printf("Degrees Celsius is %d", c);     /* print result */
        printf("\n");
        printf("Degrees Fahrenheit is %d", f);
        printf("\n");

        return 0;
}               // End main()

----------------------------------------------------------------------------------------------------------------------------------------

Problem 2

#include <stdio.h>      // allows use of printf and scanf functions

int main() {
        int f, x = 5;           // declare variables, t is also initialized

        printf("Enter a value for x: ");
        scanf("%d", &x);

        f = (x * x * x) + (5 * (x * x)) + (10 * (x)) + 15;      // expression evaluates "x"

        printf("The value of x is %d", f);      /* print result */
        printf("\n");

        return 0;
}               // End main()

----------------------------------------------------------------------------------------------------------------------------------------

Problem 3

#include <stdio.h>
#include <math.h>

int main() {
        double x1;
        double x2;
        double a;
        double b;
        double c;
        double d;

        printf("Enter a value for a: ");
        scanf("%lf", &a);
        printf("Enter a value for b: ");
        scanf("%lf", &b);
        printf("Enter a value for c: ");
        scanf("%lf", &c);

        d =  (b * b) - (4 * a * c)

        x1 = (-b + sqrt(d)) / (2 * a);
        x2 = (-b - sqrt(d)) / (2 * a);

        printf("%lf %lf\n", x1, x2);

        return 0;
}


//when compiling Problem 3, do:    gcc -lm filename.c -o filename
